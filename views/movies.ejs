<%-include ('header');-%>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>

            </tr>
        </thead>
        <tbody>
            <% if(data.length){ for(var i=0; i< data.length; i++) {%>
                <tr>
                    <th scope="row">
                        <%= (i+1) %>
                    </th>
                    <td>
                        <%= data[i].name%>
                    </td>
                </tr>
                <% } }else{ %>
                    <tr>
                        <td colspan="3">No user</td>
                    </tr>
                    <% } %>

        </tbody>
    </table>





    <%-include ('footer');-%>